{
  "name": "super-store",
  "version": "1.0.0",
  "description": "Boost your stream elements store with two way sync with a steam inventory",
  "author": "Arthur Fiorette",
  "license": "GPL-3.0",
  "private": true,
  "repository": "git+https://github.com/ArthurFiorette/super-store.git",
  "bugs": "https://github.com/ArthurFiorette/super-store/issues",
  "homepage": "https://github.com/ArthurFiorette/super-store#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug",
    "lint": "eslint . --ext .ts",
    "prettify": "prettier --write .",
    "postinstall": "declarator",
    "stream:gen": "openapi --input 'https://stoplight.io/api/v1/projects/streamelements/kappa/nodes/api.yaml?deref=optimizedBundle' --output './src/stream/generated' --exportCore=false --exportServices=false --useOptions --client axios"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.8",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.8",
    "@nestjs/event-emitter": "^1.0.0",
    "@prisma/client": "^3.5.0",
    "axios": "^0.24.0",
    "axios-cache-interceptor": "^0.6.2",
    "currency-formatter": "^1.5.8",
    "lodash.merge": "^4.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "socket.io-client": "2.4.0",
    "steam-totp": "^2.1.1",
    "steam-tradeoffer-manager": "^2.10.3",
    "steam-user": "^4.20.0",
    "steamcommunity": "^3.43.1",
    "tmi.js": "^1.8.5",
    "typed-core": "^2.0.0"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.6",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@types/currency-formatter": "^1.5.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^16.10.1",
    "@types/socket.io-client": "1.4.36",
    "@types/steam-totp": "^2.1.0",
    "@types/steam-user": "^4.19.3",
    "@types/steamcommunity": "^3.43.1",
    "@types/tmi.js": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.11.0",
    "declarator": "^1.4.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "openapi-typescript-codegen": "^0.12.3",
    "prettier": "^2.5.0",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prisma": "^3.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
